{
  "name": "deltacfoagent-frontend-tests",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "test": "jest --coverage",
    "test:ci": "jest --coverage --runInBand"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "web_ui/static/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}

